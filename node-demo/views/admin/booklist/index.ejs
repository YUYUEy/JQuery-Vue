<%- include ../public/header.ejs%>

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-2">

            <%- include ../public/aslideleft.ejs%>
          
        </div>
        <div class="col-sm-10">
          
            <div class="panel panel-default">
               
                <div class="panel-body">
                    <form style="overflow:hidden" action="/admin/booklist/find" method="post" enctype="multipart/form-data">
                        <div class="form-group">
                        <div class="col-sm-4">
                            <input class="form-control" type="text" id="name" name="Name" placeholder="请输入名称"/>
                            </div>
                            <div class="col-sm-4">
                            <button type="submit" class="btn btn-default">开始搜索</button>
                            </div>
                            <div class="col-sm-4">                           
                                
                            </div>
                        </div>
                    </form>
                    <ul class="pagination">
                        <% for(var i=1;i<20;i++){%>
                            <li>
                                <a href="/admin/booklist/findByPage?p=<%=i%>"><%=i%></a>
                            </li>
                        <%}%>
                    </ul>
                </div>
            </div>
            
                    <div class="row">
                        <div class="col-xs-12">
                            <ul class="ace-thumbnails clearfix the_booklist">
                                <% for(var i=0;i<list.length;i++){%>
                                     <li>
                                        <p><a href="/admin/bookdetail/findDetail?t=<%=list[i].name%>">
                                            <img src="<%=list[i].images%>" />
                                        </a>
                                        </p>
                                        <span><%=list[i].name%></span>
                                    </li>
                                <%}%>
                            </ul>
                        </div>
                    </div>
           
        </div>
    </div>
</div>

</body>
</html>